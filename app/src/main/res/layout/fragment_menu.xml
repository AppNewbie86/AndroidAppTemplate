<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/paw_312322"
        android:backgroundTint="#B19F9F"
        tools:context=".MenuFragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/paw_312322"
            android:backgroundTint="#E8DEDE">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="70dp"
                android:text="What can we do"
                android:textColor="#0E0D0D"
                android:textSize="34sp"
                app:layout_constraintBottom_toTopOf="@+id/divider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/divider"
                android:layout_width="409dp"
                android:layout_height="1dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="70dp"
                android:layout_marginEnd="1dp"
                android:background="?android:attr/listDivider"
                android:backgroundTint="#9A0404"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/visitdog_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="55dp"
                android:clickable="true"
                app:layout_constraintBottom_toTopOf="@+id/chatbot_image"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider"
                app:srcCompat="@drawable/frame_33620" />

            <ImageView
                android:id="@+id/chatbot_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:clickable="true"
                app:layout_constraintBottom_toTopOf="@+id/boarding_image"
                app:layout_constraintEnd_toEndOf="@+id/visitdog_image"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="@+id/visitdog_image"
                app:layout_constraintTop_toBottomOf="@+id/visitdog_image"
                app:srcCompat="@drawable/chatbot" />

            <ImageView
                android:id="@+id/boarding_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:clickable="true"
                android:contentDescription="Here can you boarding"
                app:layout_constraintBottom_toTopOf="@+id/daycare_image"
                app:layout_constraintEnd_toEndOf="@+id/chatbot_image"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="@+id/chatbot_image"
                app:layout_constraintTop_toBottomOf="@+id/chatbot_image"
                app:srcCompat="@drawable/boarding" />

            <ImageView
                android:id="@+id/daycare_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:clickable="true"
                app:layout_constraintBottom_toTopOf="@+id/dogtraining_image"
                app:layout_constraintEnd_toEndOf="@+id/boarding_image"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="@+id/boarding_image"
                app:layout_constraintTop_toBottomOf="@+id/boarding_image"
                app:srcCompat="@drawable/daycare" />

            <ImageView
                android:id="@+id/dogtraining_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="55dp"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/daycare_image"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="@+id/daycare_image"
                app:layout_constraintTop_toBottomOf="@+id/daycare_image"
                app:srcCompat="@drawable/videos" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:foregroundGravity="center_vertical"
                app:layout_constraintBottom_toBottomOf="@+id/divider"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_baseline_arrow_back_24" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
</layout>